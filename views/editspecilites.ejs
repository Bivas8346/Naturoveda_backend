<%- include('common/head')%>
<!-- ======== Preloader =========== -->
<div id="preloader">
  <div class="spinner"></div>
</div>
<!-- ======== Preloader =========== -->

<%- include('common/sidebar')%>
<!-- ======== main-wrapper start =========== -->
<main class="main-wrapper">
  <%- include('common/navbar')%>

  <!-- ========== section start ========== -->
  <section class="section">
    <div class="container-fluid">
      <!-- ========== title-wrapper start ========== -->
      <div class="title-wrapper pt-30">
        <div class="row align-items-center">
          <!-- end col -->
          <div class="col-md-6">
            <div class="breadcrumb-wrapper">
              <nav aria-label="breadcrumb">
                <ol class="breadcrumb">
                  <li class="breadcrumb-item">
                    <a href="#0">Admin Edit Form</a>
                  </li>
                </ol>
              </nav>
            </div>
          </div>
          <!-- end col -->
        </div>
        <!-- end row -->
      </div>
      <!-- ========== title-wrapper end ========== -->
      <!-- ========== section start ========== -->
      <section class="section">
        <div class="container-fluid">
          <div class="row">
            <div class="col-lg-12">
              <div class="card-style settings-card-2 mb-30">
                <div class="title mb-30">
                  <h6>My Profile</h6>
                </div>
                <form action="/specialitesupdate" method="post">
                  <div class="row">
                    <input type="hidden" name="u_id" value="<%=specil._id%>" />
                    <!-- end col -->
                    <div class="col-12">
                      <div class="input-style-1">
                        <label>Full Name</label>
                        <input
                          type="text"
                          name="title"
                          value="<%=specil.title%>"
                        />
                      </div>
                    </div>
                    <!-- end col -->
                    <div class="col-12">
                      <button class="main-btn primary-btn btn-hover">
                        Edit
                      </button>
                    </div>
                  </div>
                </form>
              </div>
              <!-- end card -->
            </div>
            <!-- end col -->
          </div>
          <!-- end row -->
        </div>
        <!-- end container -->
      </section>
      <!-- ========== section end ========== -->
    </div>
    <!-- end container -->
  </section>
  <!-- ========== section end ========== -->
  <%- include('common/footer')%>
</main>
<%- include('common/foot')%>
