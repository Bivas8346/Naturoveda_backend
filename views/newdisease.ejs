<%- include('common/head')%>
<!-- ======== Preloader =========== -->
<div id="preloader">
  <div class="spinner"></div>
</div>
<!-- ======== Preloader =========== -->

<%- include('common/sidebar')%>

<!-- ======== main-wrapper start =========== -->
<main class="main-wrapper">
  <%- include('common/navbar')%>

  <!-- ========== table components start ========== -->
  <section class="table-components">
    <div class="container-fluid">
      <!-- ========== title-wrapper start ========== -->
      <div class="title-wrapper pt-30">
        <div class="row align-items-center">
          <div class="col-md-6">
            <div class="title">
              <a href="/formdisease" class="btn btn-success"> Add Disease </a>
            </div>
          </div>
          <!-- end col -->
          <div class="col-md-6">
            <div class="breadcrumb-wrapper">
              <nav aria-label="breadcrumb">
                <ol class="breadcrumb">
                  <li class="breadcrumb-item">
                    <a href="#0">Dashboard</a>
                  </li>
                  <li class="breadcrumb-item active" aria-current="page">
                    Team Mambers
                  </li>
                </ol>
              </nav>
            </div>
          </div>
          <!-- end col -->
        </div>
        <!-- end row -->
      </div>
      <!-- ========== title-wrapper end ========== -->

      <!-- ========== tables-wrapper start ========== -->
      <div class="tables-wrapper">
        <div class="row">
          <div class="col-lg-12">
            <div class="card-style mb-30">
              <div class="table-wrapper table-responsive">
                <table class="table">
                  <thead>
                    <tr>
                      <th class="lead-info">
                        <h6>Name</h6>
                      </th>
                      <th class="lead-email">
                        <h6>Email</h6>
                      </th>
                      <th class="lead-phone">
                        <h6>Phone No</h6>
                      </th>
                      <th class="lead-company">
                        <h6>Designation</h6>
                      </th>
                      <th>
                        <h6>Action</h6>
                      </th>
                    </tr>
                    <!-- end table row-->
                  </thead>
                  <tbody>
                    <%for(let data of disc){%>
                    <tr>
                      <td class="min-width">
                        <div class="lead">
                          <div class="lead-image">
                            <!-- <img src="<%= data.image%>" alt="Profile Image" width="100%" height="100%"/> -->
                            <p><%= data.image%></p>
                          </div>
                          <div class="lead-text">
                            <p><%= data.title %></p>
                          </div>
                        </div>
                      </td>
                      <td class="min-width">
                        <p><a href="#0"><%= data.sub_title %></a></p>
                      </td>
                      <td class="min-width">
                        <p><%= data.treatment %></p>
                      </td>
                      <td class="min-width">
                        <p><%= data.specialities %></p>
                      </td>
                      <td class="min-width">
                        <p><%= data.description %></p>
                      </td>
                      <td class="min-width">
                        <p><%= data.about_title %></p>
                      </td>
                      <td class="min-width">
                        <p><%= data.about_disease %></p>
                      </td>
                      <td class="min-width">
                        <p><%= data.risk_factor %></p>
                      </td>
                      <td class="min-width">
                        <p><%= data.disease_point1 %></p>
                      </td>
                      <td class="min-width">
                        <p><%= data.disease_point2 %></p>
                      </td>
                      <td class="min-width">
                        <p><%= data.disease_point3 %></p>
                      </td>
                      <td class="min-width">
                        <p><%= data.disease_point4 %></p>
                      </td>
                      <td class="min-width">
                        <p><%= data.disease_point5 %></p>
                      </td>
                      <td class="min-width">
                        <p><%= data.disease_point6 %></p>
                      </td>
                      <td class="min-width">
                        <p><%= data.symptoms_title %></p>
                      </td>
                      <td class="min-width">
                        <p><%= data.symptoms_1 %></p>
                      </td>
                      <td class="min-width">
                        <p><%= data.symptoms_2 %></p>
                      </td>
                      <td class="min-width">
                        <p><%= data.symptoms_3 %></p>
                      </td>
                      <td class="min-width">
                        <p><%= data.symptoms_4 %></p>
                      </td>
                      <td class="min-width">
                        <p><%= data.symptoms_5 %></p>
                      </td>
                      <td class="min-width">
                        <p><%= data.complication_title %></p>
                      </td>
                      <td class="min-width">
                        <p><%= data.complication_1 %></p>
                      </td>
                      <td class="min-width">
                        <p><%= data.complication_2 %></p>
                      </td>
                      <td class="min-width">
                        <p><%= data.complication_3 %></p>
                      </td>
                      <td class="min-width">
                        <p><%= data.complication_4 %></p>
                      </td>
                      <td class="min-width">
                        <p><%= data.complication_5 %></p>
                      </td>
                      <td class="min-width">
                        <p><%= data.healthy_title %></p>
                      </td>
                      <td class="min-width">
                        <p><%= data.healthy_tips1 %></p>
                      </td>
                      <td class="min-width">
                        <p><%= data.healthy_tips2 %></p>
                      </td>
                      <td class="min-width">
                        <p><%= data.healthy_tips3 %></p>
                      </td>
                      <td class="min-width">
                        <p><%= data.sub_heading %></p>
                      </td>
                      <td>
                        <div class="action">
                          <button class="edit">
                            <a href="/newdisease/<%= data._id%>">
                              <i class="lni lni-pencil"></i>
                            </a>
                          </button>
                          <button class="text-danger">
                            <a
                              href="/newdisease/<%=data._id%>"
                              style="color: red"
                            >
                              <i class="lni lni-trash-can"></i>
                            </a>
                          </button>
                        </div>
                      </td>
                    </tr>
                    <%}%>
                    <!-- end table row -->
                  </tbody>
                </table>
                <!-- end table -->
              </div>
            </div>
            <!-- end card -->
          </div>
          <!-- end col -->
        </div>
        <!-- end row -->
      </div>
      <!-- ========== tables-wrapper end ========== -->
    </div>
    <!-- end container -->
  </section>
  <!-- ========== table components end ========== -->

  <%- include('common/footer')%>
</main>
<!-- ======== main-wrapper end =========== -->

<%- include('common/foot')%>
